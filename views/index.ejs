<!-- include header -->
<%- include('partials/_header') %>
<!-- /include header -->

<!-- Main -->
<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex-justify-between">
      <a href="/add-emp" class="border-shadow">
        <span class="text-gradient"
          >New Employee <i class="fa fa-user" aria-hidden="true"></i
        ></span>
      </a>
    </div>
    <!-- Form -->
    <form action="/" method="POST">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Job Title</th>
            <th>Email</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <%- include('partials/_show')%>
        </tbody>
      </table>
    </form>
  </div>
</main>
<!-- /Main -->

<!-- include footer -->
<%- include('partials/_footer')%>
<!-- /include footer -->
